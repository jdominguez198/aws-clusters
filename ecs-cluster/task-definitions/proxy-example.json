[
  {
    "name": "proxy-example",
    "image": "nginx:latest",
    "essential": true,
    "networkMode": "awsvpc",
    "enableExecuteCommand": true,
    "readonlyRootFilesystem": false,
    "linuxParameters": {
      "initProcessEnabled": true
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 80,
        "hostPort": 80
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "eu-west-1",
        "awslogs-group": "ecs-app",
        "awslogs-stream-prefix": "complete-ecs"
      }
    }
  }
]
